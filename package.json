    "react-native-tab-navigator": "^0.3.3",
    "react-native-wechat": "^1.9.9",
    "react-timer-mixin": "^0.13.3"
  },
  "devDependencies": {
    "babel-jest": "19.0.0",
    "babel-preset-react-native": "1.9.1",
    "jest": "19.0.2",
    "react-test-renderer": "~15.4.1"
  },
